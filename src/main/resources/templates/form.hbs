{{#partial "content"}}
    <section class="landing-pg">
         <div class="land-left"></div>
         <div class="land-right"></div>

<div class="form">
 <form action="/Team1" method="post">
  <div class="login left flex-box">
     <h1>Welcome</h1>
      <p>Hello it's a pleasure to see you, Fill the form to  Subscribe a team</p>

  </div>
  <div class="create-account left flex-box">
    <h1>Team1 Member</h1>
    <input type="Name1" placeholder="Name1:">
    <input type="Name2" placeholder="Name2">
    <input type="Name3" placeholder="Name3">
    <input type="Name4" placeholder="Name4">
    <button class="button-animate">submit</button>
  </div>
  <div class="outer">
    <div class="inner flex-box">
      <div class="sign-in overlay-form flex-box">
           <h1>Hi</h1>
           <p>Want to add your Team1 Member</p>
           <button id="signUp">Add</button>

      </div>
      <div class="sign-up overlay-form flex-box">

        <h1>Create a Team1</h1>
                     <input type="text" name="team_Name" placeholder="Leader">
                     <input type="text" name="team_Leader" placeholder="Team1 Name">
                     <input type="text" name="email" placeholder="Email">
                     <input type="text" name="language" placeholder="language">
               <button class="button-animate">Subscribe</button>
      </div>
    </div>
  </div>
 </form>
</div>
</section>
<style>
   button:focus {
       outline: none;
   }
</style>
{{/partial}}
{{> layout.hbs}}